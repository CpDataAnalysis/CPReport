        </Field>
        <Field Name="SucceedAmount">
          <DataField>SucceedAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalAmount">
          <DataField>TotalAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PaymentDW</DataSourceName>
        <CommandText>--1.Get DailyTrans during the period
select
	Trans.MerchantNo,
	IndustryMerchant.IndustryName,
	IndustryMerchant.MerchantName,
	ISNULL(Gate.BankName, N'其它') as BankName,
	convert(decimal, SUM(Trans.SucceedTransAmount))/1000000 as SucceedAmount
into
	#PeriodSumAmount
from
	FactDailyTrans Trans
	inner join
	Table_KeyMerByIndustryForBizDept IndustryMerchant
	on
		Trans.MerchantNo = IndustryMerchant.MerchantNo
	left join
	Table_GateCategory Gate
	on
		Trans.GateNo = Gate.GateNo
where
	Trans.DailyTransDate &gt;= @StartDate
	and
	Trans.DailyTransDate &lt; DATEADD(day,1,@EndDate)
group by
	Trans.MerchantNo,
	IndustryMerchant.IndustryName,
	IndustryMerchant.MerchantName,
	ISNULL(Gate.BankName, N'其它');

--3. Get Result
select
	*
into
	#FilteredSumAmount
from
	#PeriodSumAmount
where
	BankName in (@IncludeBanks);
	--BankName in (N'工商银行');
	
select
	Merchant.IndustryName,
	Merchant.MerchantName,
	Merchant.MerchantNo,
	FilteredSumAmount.BankName,
	ISNULL(FilteredSumAmount.SucceedAmount,0) as SucceedAmount,
	(select ISNULL(SUM(SucceedAmount),0) from #PeriodSumAmount where MerchantNo = Merchant.MerchantNo) as TotalAmount
into
	#DetailResult
from
	Table_KeyMerByIndustryForBizDept Merchant	
	left join
	#FilteredSumAmount FilteredSumAmount
	on
		Merchant.MerchantNo = FilteredSumAmount.MerchantNo;

if (@ReportCategory = N'明细')
begin
	select * from #DetailResult;
end
else if (@ReportCategory = N'汇总')
begin
	select
		DetailResult.IndustryName,
		DetailResult.MerchantName,
		'0' as MerchantNo,
		DetailResult.BankName,
		SUM(DetailResult.SucceedAmount) as SucceedAmount,
		(select ISNULL(SUM(SucceedAmount),0) from #PeriodSumAmount where IndustryName = DetailResult.IndustryName and MerchantName = DetailResult.MerchantName) as TotalAmount
	from
		#DetailResult DetailResult
	group by
		DetailResult.IndustryName,
		DetailResult.MerchantName,
		DetailResult.BankName;
end
		
--4. Clear temp table
drop table #PeriodSumAmount;
drop table #DetailResult;
drop table #FilteredSumAmount;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportCategory">
            <Value>=Parameters!ReportCategory.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeBanks">
            <Value>=Parameters!IncludeBanks.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
